{
	"name": "@lesjoursfr/bin-version",
	"version": "7.1.1",
	"description": "Get the version of a binary in semver format",
	"license": "MIT",
	"repository": {
		"url": "git+https://github.com/lesjoursfr/bin-version.git"
	},
	"homepage": "https://github.com/lesjoursfr/bin-version#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/bin-version/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"engines": {
		"node": "18.x || 20.x"
	},
	"exports": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm -r .yarn/cache/ && rm yarn.lock && yarn",
		"check-lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"check-format": "prettier --check \"**/*.{ts,js,cjs,mjs,json,yml,md}\"",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --write \"**/*.{ts,js,cjs,mjs,json,yml,md}\"",
		"build": "tsc",
		"test": "NODE_OPTIONS='--loader=ts-node/esm' mocha"
	},
	"keywords": [
		"binary",
		"executable",
		"version",
		"semver",
		"semantic",
		"cli"
	],
	"type": "module",
	"dependencies": {
		"execa": "^8.0.1",
		"find-versions": "^5.1.0"
	},
	"devDependencies": {
		"@tsconfig/node18": "^18.2.2",
		"@types/mocha": "^10.0.2",
		"@types/node": "^20.8.0",
		"@typescript-eslint/eslint-plugin": "^6.7.3",
		"@typescript-eslint/parser": "^6.7.3",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"mocha": "^10.2.0",
		"prettier": "^3.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"packageManager": "yarn@3.6.3"
}
