{
	"name": "@lesjoursfr/bin-version",
	"version": "8.0.1",
	"description": "Get the version of a binary in semver format",
	"license": "MIT",
	"repository": {
		"url": "git+https://github.com/lesjoursfr/bin-version.git"
	},
	"homepage": "https://github.com/lesjoursfr/bin-version#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/bin-version/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"engines": {
		"node": "22.x || 24.x"
	},
	"exports": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/**/*",
		"src/**/*"
	],
	"scripts": {
		"freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
		"check-lint": "eslint",
		"check-format": "prettier --check .",
		"check-tsc": "tsc --noEmit",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"build": "tsc",
		"test": "NODE_OPTIONS='--loader=ts-node/esm' mocha"
	},
	"keywords": [
		"binary",
		"executable",
		"version",
		"semver",
		"semantic",
		"cli"
	],
	"type": "module",
	"dependencies": {
		"execa": "^9.6.1",
		"find-versions": "^6.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@tsconfig/node22": "^22.0.5",
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.10.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^17.0.0",
		"mocha": "^11.7.5",
		"prettier": "^3.7.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.0"
	},
	"packageManager": "yarn@4.12.0"
}
